<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Dialog"
    xtype="tabpanel">
    <items jcr:primaryType="cq:WidgetCollection">
        <smartslideshow
            jcr:primaryType="cq:Widget"
            title="Slideshow Configuration"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <backgroundcss
                    jcr:primaryType="cq:Widget"
                    fieldDescription="The theme to use for styling the slideshow"
                    fieldLabel="Slideshow Theme"
                    name="./backgroundCssId"
                    xtype="textfield"/>
                <size
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Image Size"
                    heightParameter="./slideshowHeight"
                    widthParameter="./slideshowWidth"
                    xtype="sizefield"/>
                <seconds
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Slide interval (seconds)"
                    name="./showPanels"
                    xtype="numberfield"/>
                <superimposeControls
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Superimpose Controls"
                    inputValue="true"
                    name="./verticalLayout"
                    type="checkbox"
                    xtype="selection"/>
            </items>
        </smartslideshow>
        <image1
            jcr:primaryType="cq:Widget"
            cropParameter="./image1/imageCrop"
            ddGroups="[media]"
            fileNameParameter="./image1/fileName"
            fileReferenceParameter="./image1/fileReference"
            mapParameter="./image1/imageMap"
            name="./image1/file"
            requestSuffix="/image1.img.png"
            rotateParameter="./image1/imageRotate"
            sizeLimit="100"
            title="Image 1"
            uploadUrl="/tmp/upload/*"
            xtype="smartimage"/>
        <advanced1
            jcr:primaryType="cq:Widget"
            title="Image 1 Properties"
            xtype="panel">
            <items jcr:primaryType="cq:WidgetCollection">
                <title
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Title"
                    name="./image1/jcr:title"
                    xtype="textfield"/>
                <alt
                    jcr:primaryType="cq:Widget"
                    fieldDescription="(leave empty to use the title defined above)"
                    fieldLabel="Alt Text"
                    name="./image1/alt"
                    xtype="textfield"/>
                <linkURL
                    jcr:primaryType="cq:Widget"
                    fieldDescription="Drop files or pages from the Content Finder"
                    fieldLabel="Link to"
                    name="./image1/linkURL"
                    xtype="pathfield"/>
                <size
                    jcr:primaryType="cq:Widget"
                    fieldLabel="Size"
                    heightParameter="./image1/height"
                    widthParameter="./image1/width"
                    xtype="sizefield"/>
            </items>
        </advanced1>
